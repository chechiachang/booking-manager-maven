<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" 
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee 
http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <context-param>
        <param-name>javax.faces.PROJECT_STAGE</param-name>
        <param-value>Development</param-value>
    </context-param>
    <context-param>
        <param-name>javax.faces.CONFIG_FILES</param-name>
        <param-value>/WEB-INF/faces-config.xml</param-value>
    </context-param>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>com.ccc.mavenbmcp.action.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>com.ccc.mavenbmcp.action.LogoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserServlet</servlet-name>
        <servlet-class>com.ccc.mavenbmcp.servlet.UserServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetAllObjectClassesAction</servlet-name>
        <servlet-class>com.ccc.mavenbmcp.action.GetAllObjectClassesAction</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetAllRoomAction</servlet-name>
        <servlet-class>com.ccc.mavenbmcp.action.GetAllRoomAction</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetAllFloorAction</servlet-name>
        <servlet-class>com.ccc.smartfloorplan.action.GetAllFloorAction</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EventJsonServlet</servlet-name>
        <servlet-class>com.ccc.mavenbmcp.servlet.EventJsonServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RoomEventJsonServlet</servlet-name>
        <servlet-class>com.ccc.mavenbmcp.servlet.RoomEventJsonServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetAllRoomEvent</servlet-name>
        <servlet-class>com.ccc.mavenbmcp.action.GetAllRoomEvent</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewRoomDivAction</servlet-name>
        <servlet-class>com.ccc.mavenbmcp.action.NewRoomDivAction</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetAllUserAction</servlet-name>
        <servlet-class>com.ccc.mavenbmcp.action.GetAllUserAction</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddUserAction</servlet-name>
        <servlet-class>com.ccc.mavenbmcp.action.AddUserAction</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditUserAction</servlet-name>
        <servlet-class>com.ccc.mavenbmcp.action.EditUserAction</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CheckUserNameAction</servlet-name>
        <servlet-class>com.ccc.mavenbmcp.action.CheckUserNameAction</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteUserAction</servlet-name>
        <servlet-class>com.ccc.mavenbmcp.action.DeleteUserAction</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CheckRoomEventTimeAction</servlet-name>
        <servlet-class>com.ccc.mavenbmcp.action.CheckRoomEventTimeAction</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateRoomAction</servlet-name>
        <servlet-class>com.ccc.mavenbmcp.action.UpdateRoomAction</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetUserInfoAction</servlet-name>
        <servlet-class>com.ccc.mavenbmcp.action.GetUserInfoAction</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetRoomFootpathAction</servlet-name>
        <servlet-class>com.ccc.mavenbmcp.action.GetRoomFootpathAction</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditUserPasswordAction</servlet-name>
        <servlet-class>com.ccc.mavenbmcp.action.EditUserPasswordAction</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetRoomInfoJsonAction</servlet-name>
        <servlet-class>com.ccc.mavenbmcp.action.GetRoomInfoJsonAction</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Faces Servlet</servlet-name>
        <servlet-class>javax.faces.webapp.FacesServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>Faces Servlet</servlet-name>
        <url-pattern>*.jsf</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Faces Servlet</servlet-name>
        <url-pattern>*.faces</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/LogoutServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetAllObjectClassesAction</servlet-name>
        <url-pattern>/GetAllObjectClassesAction</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetAllRoomAction</servlet-name>
        <url-pattern>/GetAllRoomAction</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetAllFloorAction</servlet-name>
        <url-pattern>/GetAllFloorAction</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EventJsonServlet</servlet-name>
        <url-pattern>/EventJsonServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RoomEventJsonServlet</servlet-name>
        <url-pattern>/RoomEventJsonServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserServlet</servlet-name>
        <url-pattern>/UserServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetAllRoomEvent</servlet-name>
        <url-pattern>/GetAllRoomEvent</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewRoomDivAction</servlet-name>
        <url-pattern>/NewRoomDivAction</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetAllUserAction</servlet-name>
        <url-pattern>/GetAllUserAction</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddUserAction</servlet-name>
        <url-pattern>/AddUserAction</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditUserAction</servlet-name>
        <url-pattern>/EditUserAction</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CheckUserNameAction</servlet-name>
        <url-pattern>/CheckUserNameAction</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteUserAction</servlet-name>
        <url-pattern>/DeleteUserAction</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CheckRoomEventTimeAction</servlet-name>
        <url-pattern>/CheckRoomEventTimeAction</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateRoomAction</servlet-name>
        <url-pattern>/UpdateRoomAction</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetUserInfoAction</servlet-name>
        <url-pattern>/GetUserInfoAction</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetRoomFootpathAction</servlet-name>
        <url-pattern>/GetRoomFootpathAction</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditUserPasswordAction</servlet-name>
        <url-pattern>/EditUserPasswordAction</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetRoomInfoJsonAction</servlet-name>
        <url-pattern>/GetRoomInfoJsonAction</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>floorplan.jsp</welcome-file>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>
